<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="ageVM" type="com.front_end.app.modules.age.data.viewmodel.AgeVM"/>
    </data>
    <FrameLayout android:id="@+id/frameAge"
        android:layout_height="match_parent"
        android:layout_width="match_parent" android:background="@drawable/img_360f247848348">
        <LinearLayout android:id="@+id/linearColumnone"
            android:layout_gravity="top|center"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_19pxv"
            android:layout_marginEnd="@dimen/_39pxh"
            android:layout_marginStart="@dimen/_39pxh"
            android:layout_marginTop="@dimen/_19pxv"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_width="match_parent" android:orientation="vertical">
            <TextView android:ellipsize="end" android:id="@+id/txtOne"
                android:layout_gravity="center"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_100pxh"
                android:layout_marginStart="@dimen/_100pxh"
                android:layout_width="wrap_content"
                android:singleLine="true"
                android:text="@{ageVM.ageModel.txtOne}"
                app:layout_constraintTop_toBottomOf="@id/txtOne"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                style="@style/txtPoppinsbold24" tools:text="@string/lbl"/>
            <TextView android:ellipsize="end" android:gravity="center"
                android:id="@+id/txtWhatsyourage"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_23pxh"
                android:layout_marginStart="@dimen/_23pxh"
                android:layout_marginTop="@dimen/_45pxv"
                android:layout_width="match_parent"
                android:singleLine="true"
                app:layout_constraintTop_toBottomOf="@id/txtOne"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                android:text="@{ageVM.ageModel.txtWhatsyourage}"
                style="@style/txt_2" tools:text="@string/msg_what_s_your_age"/>
            <EditText  android:background="@drawable/rectangle_bg_gray_201_radius_17_5"
                android:gravity="center_horizontal"
                android:ellipsize="end" android:hint="@string/lbl_age"
                android:id="@+id/etRectangleTwentyOne"
                android:inputType="number|none"
                android:paddingHorizontal="@dimen/_3pxh"
                android:paddingVertical="@dimen/_1pxv"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_23pxh"
                android:layout_marginStart="@dimen/_23pxh"
                android:layout_marginTop="@dimen/_22pxv"
                android:layout_width="match_parent"
                android:singleLine="true"
                app:layout_constraintTop_toBottomOf="@id/txtWhatsyourage"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                android:text="@={ageVM.ageModel.etRectangleTwentyOneValue}"
                android:textColorHint="@color/gray_703"
                style="@style/ThemeOverlay.Material3.TextInputEditText.FilledBox.Dense"/>
            <TextView android:gravity="center_horizontal"
                android:id="@+id/txtWeusethisdat"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_23pxh"
                android:layout_marginStart="@dimen/_23pxh"
                android:layout_marginTop="@dimen/_82pxv"
                android:layout_width="match_parent"
                app:layout_constraintTop_toBottomOf="@id/etRectangleTwentyOne"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                android:text="@{ageVM.ageModel.txtWeusethisdat}"
                style="@style/txt_3" tools:text="@string/msg_we_use_this_dat"/>
            <LinearLayout android:id="@+id/linearRowback"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_63pxv"
                android:layout_width="match_parent" android:orientation="horizontal">
                <TextView android:ellipsize="end"
                    android:id="@+id/txtBack"
                    android:background="@drawable/rectangle_bg_gray_201_radius_17_5"
                    android:gravity="center"
                    android:paddingHorizontal="@dimen/_3pxh"
                    android:paddingVertical="@dimen/_1pxv"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_5pxv"
                    android:layout_marginStart="@dimen/_41pxh"
                    android:layout_marginTop="@dimen/_4pxv"
                    android:layout_weight="0.52"
                    app:layout_constraintTop_toBottomOf="@id/txtWeusethisdat"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintEnd_toStartOf="@id/txtNext"
                    android:layout_width="10dp" android:singleLine="true"
                    android:text="@{ageVM.ageModel.txtBack}"
                    style="@style/txtPoppinsbold24_1" tools:text="@string/lbl_back"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtNext"
                    android:background="@drawable/rectangle_bg_gray_201_radius_17_5"
                    android:gravity="center"
                    android:paddingHorizontal="@dimen/_5pxh"
                    android:paddingVertical="@dimen/_1pxv"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_2pxv"
                    android:layout_marginEnd="@dimen/_46pxh"
                    android:layout_marginStart="@dimen/_25pxh"
                    android:layout_marginTop="@dimen/_4pxv"
                    android:layout_weight="0.52"
                    app:layout_constraintTop_toBottomOf="@id/txtWeusethisdat"
                    app:layout_constraintStart_toEndOf="@id/txtBack"
                    app:layout_constraintEnd_toEndOf="parent"
                    android:layout_width="10dp" android:singleLine="true"
                    android:text="@{ageVM.ageModel.txtNext}"
                    style="@style/txtPoppinsbold24_1" tools:text="@string/lbl_next"/>
            </LinearLayout>
        </LinearLayout>
    </FrameLayout>
</layout>