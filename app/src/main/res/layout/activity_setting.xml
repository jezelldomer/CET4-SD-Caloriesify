<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="settingVM" type="com.front_end.app.modules.setting.data.viewmodel.SettingVM"/>
    </data>
    <LinearLayout android:gravity="center"
        android:id="@+id/linearSetting"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylegray_200">
        <androidx.appcompat.widget.Toolbar android:gravity="start"
            android:id="@+id/toolbarToolbar"
            android:layout_height="wrap_content"
            app:layout_constraintTop_toBottomOf="@+id/toolbarToolbar"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_width="match_parent" app:contentInsetStart="0dp">
            <LinearLayout android:gravity="start"
                android:id="@+id/linearRowarrowfive"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:orientation="horizontal" style="@style/groupStylegreen_A700">
                <LinearLayout android:gravity="center_horizontal"
                    android:id="@+id/linearColumnarrowfive"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_7pxv"
                    android:layout_marginStart="@dimen/_4pxh"
                    android:layout_marginTop="@dimen/_4pxv"
                    android:layout_weight="0.19"
                    android:layout_width="0dp"
                    app:layout_constraintTop_toBottomOf="@+id/linearColumnarrowfive"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    android:orientation="vertical" >
                    <ImageView android:id="@+id/imageArrowFive"
                        android:layout_gravity="center"
                        android:layout_height="@dimen/_15pxh"
                        android:layout_marginBottom="@dimen/_17pxv"
                        android:layout_marginEnd="@dimen/_15pxh"
                        android:layout_marginStart="@dimen/_15pxh"
                        android:layout_marginTop="@dimen/_17pxv"
                        android:layout_width="@dimen/_10pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_arrow5"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_arrow5"/>
                </LinearLayout>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtAccountSetting"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_9pxv"
                    android:layout_marginEnd="@dimen/_119pxh"
                    android:layout_marginStart="@dimen/_68pxh"
                    android:layout_marginTop="@dimen/_15pxv"
                    android:layout_weight="0.81"
                    app:layout_constraintTop_toBottomOf="@+id/txtAccountSetting"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    android:layout_width="0dp" android:singleLine="true"
                    android:text="@{settingVM.settingModel.txtAccountSetting}"
                    style="@style/txtSFCompactDisplaybold24" tools:text="@string/msg_account_setting"/>
            </LinearLayout>
        </androidx.appcompat.widget.Toolbar>
        <LinearLayout android:id="@+id/linearColumnprofile"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_44pxh"
            android:layout_marginStart="@dimen/_44pxh"
            android:layout_marginTop="@dimen/_27pxv"
            android:layout_width="match_parent"
            app:layout_constraintTop_toBottomOf="@+id/linearColumnprofile"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            android:orientation="vertical" style="@style/groupStylebluegray_3">
            <TextView android:ellipsize="end"
                android:id="@+id/txtProfile"
                android:layout_gravity="center_horizontal"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_14pxh"
                android:layout_marginStart="@dimen/_14pxh"
                android:layout_marginTop="@dimen/_11pxv"
                android:layout_width="wrap_content"
                app:layout_constraintTop_toBottomOf="@+id/txtProfile"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                android:singleLine="true"
                android:text="@{settingVM.settingModel.txtProfile}"
                style="@style/txt_11" tools:text="@string/lbl_profile"/>
            <FrameLayout android:id="@+id/frameStackellipsetwentyfive"
                android:layout_gravity="center_horizontal"
                android:layout_height="@dimen/_140pxv"
                android:layout_marginEnd="@dimen/_14pxh"
                android:layout_marginStart="@dimen/_14pxh"
                app:layout_constraintTop_toBottomOf="@+id/frameStackellipsetwentyfive"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                android:layout_marginTop="@dimen/_5pxv" android:layout_width="@dimen/_150pxh">

                <View
                    android:id="@+id/viewEllipseTwentyFive"
                    android:layout_width="@dimen/_85pxh"
                    android:layout_height="80dp"
                    android:layout_gravity="center"
                    android:background="@drawable/rectangle_bg_white_a700_radius_40_5" />
                <ImageView android:id="@+id/imageUser"
                    android:layout_gravity="center"
                    android:layout_height="@dimen/_50pxh"
                    android:layout_marginBottom="@dimen/_22pxv"
                    android:layout_marginTop="@dimen/_21pxv"
                    android:layout_width="@dimen/_50pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_user_black_900"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_user_black_900"/>
            </FrameLayout>
            <TextView android:ellipsize="end"
                android:id="@+id/txtNamePirate"
                android:layout_gravity="center_horizontal"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_14pxh"
                android:layout_marginStart="@dimen/_14pxh"
                android:layout_marginTop="@dimen/_15pxv"
                app:layout_constraintTop_toBottomOf="@+id/txtNamePirate"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                android:layout_width="wrap_content"
                android:singleLine="true"
                android:text="@{settingVM.settingModel.txtNamePirate}"
                style="@style/txtSFCompactDisplayregular11" tools:text="@string/lbl_name_pirate"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtAgeCounter"
                android:layout_gravity="start"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_144pxh"
                app:layout_constraintTop_toBottomOf="@+id/txtAgeCounter"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                android:layout_marginStart="@dimen/_144pxh"
                android:layout_marginTop="@dimen/_9pxv"
                android:layout_width="wrap_content"
                android:singleLine="true"
                android:text="@{settingVM.settingModel.txtAgeCounter}"
                style="@style/txtSFCompactDisplayregular11" tools:text="@string/lbl_age_20"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtEmaildatapir"
                android:layout_gravity="end"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_84pxh"
                android:layout_marginStart="@dimen/_84pxh"
                android:layout_marginTop="@dimen/_7pxv"
                android:layout_width="wrap_content"
                app:layout_constraintTop_toBottomOf="@+id/txtEmaildatapir"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                android:singleLine="true"
                android:text="@{settingVM.settingModel.txtEmaildatapir}"
                style="@style/txtSFCompactDisplayregular11" tools:text="@string/msg_email_data_pir"/>
            <View android:background="@color/gray_400"
                android:id="@+id/lineLineTwo"
                android:layout_gravity="center_horizontal"
                android:layout_height="@dimen/_1pxv"
                android:layout_marginEnd="@dimen/_12pxh"
                android:layout_marginStart="@dimen/_14pxh"
                android:layout_marginTop="@dimen/_14pxv" android:layout_width="@dimen/_312pxh"/>
            <LinearLayout android:gravity="start"
                android:id="@+id/linearRowcurrentweight"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_marginTop="@dimen/_28pxv"
                app:layout_constraintTop_toBottomOf="@+id/linearRowcurrentweight"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                android:layout_width="wrap_content" android:orientation="horizontal">
                <TextView android:ellipsize="end"
                    android:id="@+id/txtCurrentweight"
                    android:layout_height="wrap_content"
                    android:layout_width="90dp" android:singleLine="true"
                    android:text="@{settingVM.settingModel.txtCurrentweight}"
                    style="@style/txtSFCompactDisplaylight11" tools:text="@string/lbl_current_weight"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtWeight"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_80pxh"
                    android:layout_weight="0.6"
                    android:layout_width="0dp" android:singleLine="true"
                    android:text="@{settingVM.settingModel.txtWeight}"
                    style="@style/txtSFCompactDisplaylight11" tools:text="@string/lbl_80_kg"/>
            </LinearLayout>
            <LinearLayout android:id="@+id/linearRowgoal"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_marginTop="@dimen/_32pxv"
                app:layout_constraintTop_toBottomOf="@+id/linearRowgoal"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                android:layout_width="wrap_content" android:orientation="horizontal">
                <TextView android:ellipsize="end"
                    android:id="@+id/txtGoal"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_1pxv"
                    android:layout_width="50dp" android:singleLine="true"
                    android:text="@{settingVM.settingModel.txtGoal}"
                    style="@style/txtSFCompactDisplaylight11" tools:text="@string/lbl_goal"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtLoseWeight"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_90pxh"
                    android:layout_marginTop="@dimen/_1pxv"
                    android:layout_weight="0.73"
                    android:layout_width="0dp" android:singleLine="true"
                    android:text="@{settingVM.settingModel.txtLoseWeight}"
                    style="@style/txtSFCompactDisplaylight11" tools:text="@string/lbl_lose_weight"/>
            </LinearLayout>
            <LinearLayout android:gravity="start"
                android:id="@+id/linearRowtargetweight"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_49pxv"
                android:layout_gravity="center"
                app:layout_constraintTop_toBottomOf="@+id/linearRowtargetweight"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                android:layout_marginTop="@dimen/_36pxv"
                android:layout_width="wrap_content" android:orientation="horizontal">
                <TextView android:ellipsize="end"
                    android:id="@+id/txtTargetWeight"
                    android:layout_height="wrap_content"
                    android:layout_width="90dp" android:singleLine="true"
                    android:text="@{settingVM.settingModel.txtTargetWeight}"
                    style="@style/txtSFCompactDisplaylight11" tools:text="@string/lbl_target_weight"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtWeightOne"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_80pxh"
                    android:layout_weight="0.30"
                    android:layout_width="0dp" android:singleLine="true"
                    android:text="@{settingVM.settingModel.txtWeightOne}"
                    style="@style/txtSFCompactDisplaylight11" tools:text="@string/lbl_69_kg"/>
            </LinearLayout>
        </LinearLayout>
        <androidx.appcompat.widget.AppCompatButton
            android:ellipsize="end" android:gravity="center"
            android:id="@+id/btnCustomization"
            app:layout_constraintTop_toBottomOf="@+id/btnCustomization"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_gravity="center_horizontal"
            android:layout_height="@dimen/_38pxv"
            android:layout_marginEnd="@dimen/_44pxh"
            android:layout_marginStart="@dimen/_44pxh"
            android:layout_marginTop="@dimen/_63pxv"
            android:layout_width="@dimen/_167pxh"
            android:paddingBottom="@dimen/_9pxv"
            android:paddingEnd="@dimen/_30pxh"
            android:paddingStart="@dimen/_30pxh"
            android:paddingTop="@dimen/_9pxv" android:singleLine="true"
            android:text="@string/lbl_customization"
            android:textAllCaps="false" style="@style/btnSolidRounded_2" tools:text="@string/lbl_customization"/>
        <LinearLayout android:gravity="start"
            android:id="@+id/linearRowlinethree"
            android:layout_height="wrap_content"
            app:layout_constraintTop_toBottomOf="@+id/linearRowlinethree"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginBottom="@dimen/_93pxv"
            android:layout_marginEnd="@dimen/_42pxh"
            android:layout_marginStart="@dimen/_44pxh"
            android:layout_marginTop="@dimen/_40pxv"
            android:layout_width="match_parent"
            android:orientation="horizontal" style="@style/groupStylebluegray_4">
            <LinearLayout android:gravity="start"
                android:id="@+id/linearColumnlinethree"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_5pxv"
                android:layout_marginStart="@dimen/_8pxh"
                android:layout_marginTop="@dimen/_10pxv"
                android:layout_weight="0.99" android:layout_width="0dp" android:orientation="vertical">
                <View android:background="@color/gray_400"
                    android:id="@+id/lineLineThree"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="@dimen/_1pxv"
                    android:layout_marginEnd="@dimen/_27pxh"
                    android:layout_marginStart="@dimen/_38pxh" android:layout_width="@dimen/_250pxh"/>
                <LinearLayout android:gravity="start"
                    android:id="@+id/linearRowuserOne"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:layout_marginTop="@dimen/_18pxv"
                    android:layout_width="wrap_content" android:orientation="horizontal">
                    <ImageView android:id="@+id/imageUserOne"
                        android:layout_height="@dimen/_22pxh"
                        android:layout_width="@dimen/_24pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_user_black_900"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_user_black_900"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtPersonalDetail"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_4pxv"
                        android:layout_marginStart="@dimen/_11pxh"
                        android:layout_marginTop="@dimen/_4pxv"
                        android:layout_width="match_parent"
                        android:singleLine="true"
                        android:text="@{settingVM.settingModel.txtPersonalDetail}"
                        style="@style/txtSFCompactDisplaylight11" tools:text="@string/msg_personal_detail"/>
                </LinearLayout>

                <View android:background="@color/gray_400"
                    android:id="@+id/lineLineSix"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="@dimen/_1pxv"
                    android:layout_marginEnd="@dimen/_27pxh"
                    android:layout_marginStart="@dimen/_38pxh"
                    android:layout_marginTop="@dimen/_7pxv" android:layout_width="@dimen/_250pxh"/>
            </LinearLayout>
            <ImageView android:id="@+id/imageArrowEight"
                android:layout_height="@dimen/_20pxh"
                android:layout_marginBottom="@dimen/_130pxv"
                android:layout_marginTop="@dimen/_33pxv"
                android:layout_width="@dimen/_30pxh"

                tools:ignore="ContentDescription" tools:src="@drawable/img_arrow8"/>

        </LinearLayout>
    </LinearLayout>
</layout>
