<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="obeseVM" type="com.front_end.app.modules.obese.data.viewmodel.ObeseVM"/>
    </data>
    <LinearLayout android:gravity="center_horizontal"
        android:id="@+id/linearObese"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylegray_200">
        <LinearLayout android:gravity="center_horizontal"
            android:id="@+id/linearMainEclipse"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_64pxh"
            android:layout_marginStart="@dimen/_64pxh"
            android:layout_marginTop="@dimen/_105pxv"
            android:layout_width="match_parent"
            android:orientation="vertical" style="@style/groupStylegray_200_16cornerRadius">
            <FrameLayout android:id="@+id/frameStackellipseten"
                android:layout_gravity="center_horizontal"
                android:layout_height="@dimen/_250pxv"
                android:background="@drawable/rectangle_border_gray_604_radius_109"
                android:layout_width="@dimen/_218pxh">
                <View
                    android:background="@drawable/rectangle_border_red_900_radius_109"
                    android:id="@+id/viewEllipseTen"
                    android:layout_gravity="center"
                    android:layout_height="@dimen/_200pxh" android:layout_width="@dimen/_200pxh"/>
                <TextView android:ellipsize="end"
                    android:gravity="top|start"
                    android:id="@+id/txtObsesse"
                    android:layout_gravity="center"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_40pxv"
                    android:layout_marginEnd="@dimen/_31pxh"
                    android:layout_marginStart="@dimen/_33pxh"
                    android:layout_marginTop="@dimen/_40pxv"
                    android:layout_width="match_parent"
                    android:singleLine="true"
                    style="@style/txtPoppinssemibold36" tools:text="@string/lbl_obsesse"/>
            </FrameLayout>
        </LinearLayout>
        <TextView android:ellipsize="end"
            android:gravity="center_horizontal"
            android:id="@+id/txtYouhaveObese"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_55pxh"
            android:layout_marginStart="@dimen/_64pxh"
            android:layout_marginTop="@dimen/_45pxv"
            android:layout_width="match_parent"
            android:singleLine="true"
            android:text="@{obeseVM.obeseModel.txtYouhaveObese}"
            style="@style/txt_14" tools:text="@string/msg_you_have_obese"/>
        <androidx.appcompat.widget.AppCompatButton
            android:ellipsize="end" android:gravity="center"
            android:id="@+id/btnDetails"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_64pxh"
            android:layout_marginStart="@dimen/_64pxh"
            android:layout_marginTop="@dimen/_233pxv"
            android:layout_width="match_parent"
            android:paddingBottom="@dimen/_14pxv"
            android:paddingEnd="@dimen/_30pxh"
            android:paddingStart="@dimen/_30pxh"
            android:paddingTop="@dimen/_14pxv" android:singleLine="true"
            android:text="@string/lbl_details"
            android:textAllCaps="false" style="@style/btnSolidRounded_6" tools:text="@string/lbl_details"/>
        <androidx.appcompat.widget.AppCompatButton
            android:ellipsize="end" android:gravity="center"
            android:id="@+id/btnBack"
            android:layout_gravity="center_horizontal"
            android:layout_height="@dimen/_46pxv"
            android:layout_marginBottom="@dimen/_20pxv"
            android:layout_marginEnd="@dimen/_64pxh"
            android:layout_marginStart="@dimen/_64pxh"
            android:layout_marginTop="@dimen/_18pxv"
            android:layout_width="@dimen/_228pxh"
            android:paddingBottom="@dimen/_5pxv"
            android:paddingEnd="@dimen/_30pxh"
            android:paddingStart="@dimen/_30pxh"
            android:paddingTop="@dimen/_5pxv" android:singleLine="true"
            android:text="@string/lbl_back2" android:textAllCaps="false"
            style="@style/btnSolidRounded_7" tools:text="@string/lbl_back2"/>
    </LinearLayout>
</layout>
