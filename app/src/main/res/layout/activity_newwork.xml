<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="newworkVM" type="com.front_end.app.modules.newwork.data.viewmodel.NewworkVM"/>
    </data>
    <LinearLayout android:id="@+id/linearNewwork"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylegray_200">
        <FrameLayout android:id="@+id/frameStackarrowfive"
            android:layout_gravity="start"
            android:layout_height="@dimen/_39pxv"
            android:layout_marginEnd="@dimen/_12pxh"
            android:layout_marginStart="@dimen/_12pxh"
            android:layout_marginTop="@dimen/_24pxv"
            android:layout_width="@dimen/_41pxh" style="@style/groupStylegreen_A700cornerRadius">
            <ImageView android:id="@+id/imageArrowFive"
                android:layout_gravity="bottom|start"
                android:layout_height="@dimen/_3pxh"
                android:layout_marginBottom="@dimen/_17pxv"
                android:layout_marginEnd="@dimen/_15pxh"
                android:layout_marginStart="@dimen/_15pxh"
                android:layout_marginTop="@dimen/_17pxv"
                android:layout_width="@dimen/_1pxh"
                android:scaleType="fitXY"
                android:src="@drawable/img_arrow5"
                tools:ignore="ContentDescription" tools:src="@drawable/img_arrow5"/>
        </FrameLayout>
        <LinearLayout android:id="@+id/linearRowarrowleft"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_12pxh"
            android:layout_marginStart="@dimen/_8pxh"
            android:layout_marginTop="@dimen/_23pxv"
            android:layout_width="match_parent" android:orientation="horizontal">
            <ImageView android:id="@+id/imageArrowleft"
                android:layout_height="@dimen/_20pxh"
                android:layout_marginBottom="@dimen/_1pxv"
                android:layout_marginTop="@dimen/_1pxv"
                android:layout_width="@dimen/_11pxh"
                android:scaleType="fitXY"
                android:src="@drawable/img_arrowleft"
                tools:ignore="ContentDescription" tools:src="@drawable/img_arrowleft"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtUntitled"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_163pxh"
                android:layout_weight="0.8" android:layout_width="0dp"
                android:singleLine="true"
                android:text="@{newworkVM.newworkModel.txtUntitled}"
                style="@style/txtRobotobold18" tools:text="@string/lbl_untitled"/>
            <ImageView android:id="@+id/imageFrameTen"
                android:layout_height="@dimen/_20pxh"
                android:layout_marginBottom="@dimen/_1pxv"
                android:layout_marginStart="@dimen/_163pxh"
                android:layout_marginTop="@dimen/_1pxv"
                android:layout_width="@dimen/_4pxh"
                android:scaleType="fitXY"
                android:src="@drawable/img_iconhamburge"
                tools:ignore="ContentDescription" tools:src="@drawable/img_iconhamburge"/>
        </LinearLayout>
        <TextView android:ellipsize="end" android:id="@+id/txtTime"
            android:layout_gravity="start"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_20pxh"
            android:layout_marginStart="@dimen/_20pxh"
            android:layout_marginTop="@dimen/_43pxv"
            android:layout_width="wrap_content"
            android:singleLine="true"
            android:text="@{newworkVM.newworkModel.txtTime}"
            style="@style/txtRobotomedium18" tools:text="@string/lbl_time"/>
        <EditText android:background="@android:color/transparent"
            android:ellipsize="end" android:hint="@string/lbl_activity"
            android:id="@+id/etActivity" android:inputType="text"
            android:layout_gravity="start"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_20pxh"
            android:layout_marginStart="@dimen/_20pxh"
            android:layout_marginTop="@dimen/_7pxv"
            android:layout_width="wrap_content"
            android:singleLine="true"
            android:text="@={newworkVM.newworkModel.etActivityValue}"
            android:textColorHint="@color/black_900"
            style="@style/etRobotomedium18" tools:ignore="Autofill"/>
        <EditText android:background="@android:color/transparent"
            android:ellipsize="end" android:hint="@string/lbl_person"
            android:id="@+id/etWith" android:inputType="text"
            android:layout_gravity="start"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_20pxv"
            android:layout_marginEnd="@dimen/_22pxh"
            android:layout_marginStart="@dimen/_22pxh"
            android:layout_marginTop="@dimen/_3pxv"
            android:layout_width="wrap_content"
            android:singleLine="true"
            android:text="@={newworkVM.newworkModel.etWithValue}"
            android:textColorHint="@color/black_900"
            style="@style/etRobotomedium18" tools:ignore="Autofill"/>
    </LinearLayout>
</layout>
