<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="oldVM" type="com.front_end.app.modules.old.data.viewmodel.OldVM"/>
    </data>
    <LinearLayout android:gravity="start" android:id="@+id/linearOld"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylegray_200">
        <androidx.appcompat.widget.Toolbar
            android:gravity="center_horizontal"
            android:id="@+id/toolbarToolbar"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_8pxh"
            android:layout_marginStart="@dimen/_14pxh"
            android:layout_marginTop="@dimen/_21pxv"
            android:layout_width="match_parent" app:contentInsetStart="0dp">
            <LinearLayout android:id="@+id/linearRowarrowleft"
                android:layout_height="wrap_content"
                android:layout_width="match_parent" android:orientation="horizontal">
                <ImageView android:id="@+id/imageArrowleft"
                    android:layout_height="@dimen/_20pxh"
                    android:layout_marginBottom="@dimen/_1pxv"
                    android:layout_marginTop="@dimen/_1pxv"
                    android:layout_width="@dimen/_11pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_arrowleft"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_arrowleft"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtNOTESNAME"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_144pxh"
                    android:layout_weight="0.86"
                    android:layout_width="0dp" android:singleLine="true"
                    android:text="@{oldVM.oldModel.txtNOTESNAME}"
                    style="@style/txtRobotobold18" tools:text="@string/lbl_notes_name2"/>
                <ImageView android:id="@+id/imageFrameTen"
                    android:layout_height="@dimen/_20pxh"
                    android:layout_marginBottom="@dimen/_1pxv"
                    android:layout_marginStart="@dimen/_144pxh"
                    android:layout_marginTop="@dimen/_1pxv"
                    android:layout_width="@dimen/_4pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_iconhamburge"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_iconhamburge"/>
            </LinearLayout>
        </androidx.appcompat.widget.Toolbar>
        <EditText android:background="@android:color/transparent"
            android:ellipsize="end" android:hint="@string/lbl_subject"
            android:id="@+id/etUntitled" android:inputType="text"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_14pxh"
            android:layout_marginStart="@dimen/_14pxh"
            android:layout_marginTop="@dimen/_26pxv"
            android:layout_width="wrap_content"
            android:singleLine="true"
            android:text="@={oldVM.oldModel.etUntitledValue}"
            android:textColorHint="@color/black_900"
            style="@style/etRobotobold24" tools:ignore="Autofill"/>
        <EditText android:background="@android:color/transparent"
            android:ellipsize="end" android:hint="@string/lbl_content"
            android:id="@+id/etContent" android:inputType="text"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_20pxv"
            android:layout_marginEnd="@dimen/_20pxh"
            android:layout_marginStart="@dimen/_20pxh"
            android:layout_width="wrap_content"
            android:singleLine="true"
            android:text="@={oldVM.oldModel.etContentValue}"
            android:textColorHint="@color/black_900"
            style="@style/etRobotomedium18" tools:ignore="Autofill"/>
    </LinearLayout>
</layout>
