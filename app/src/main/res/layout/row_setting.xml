<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="settingRowModel" type="com.front_end.app.modules.setting.data.model.SettingRowModel"/>
    </data>
    <LinearLayout android:id="@+id/linearRowgroup105"
        android:layout_height="wrap_content"
        android:layout_width="match_parent" android:orientation="horizontal">
        <ImageView android:id="@+id/imageGroup105"
            android:layout_height="@dimen/_24pxh"
            android:layout_marginTop="@dimen/_1pxv"
            android:layout_width="@dimen/_25pxh"
            android:scaleType="fitXY"
            android:src="@drawable/img_group105"
            tools:ignore="ContentDescription" tools:src="@drawable/img_group105"/>
        <LinearLayout android:gravity="start"
            android:id="@+id/linearColumnadjustmacronut"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_1pxv"
            android:layout_marginStart="@dimen/_11pxh"
            android:layout_weight="0.81" android:layout_width="0dp" android:orientation="vertical">
            <TextView android:ellipsize="end"
                android:gravity="center_horizontal"
                android:id="@+id/txtAdjustMacroNut"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_2pxh"
                android:layout_width="match_parent"
                android:singleLine="true"
                android:text="@{settingRowModel.txtAdjustMacroNut}"
                style="@style/txtSFCompactDisplaylight11" tools:text="@string/msg_adjust_macronut"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtCarbsProtein"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_10pxh"
                android:layout_marginTop="@dimen/_1pxv"
                android:layout_width="match_parent"
                android:singleLine="true"
                android:text="@{settingRowModel.txtCarbsProtein}"
                style="@style/txtSFCompactDisplayultralight7" tools:text="@string/msg_carbs_protein"/>
        </LinearLayout>
    </LinearLayout>
</layout>
