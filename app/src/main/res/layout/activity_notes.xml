                    android:layout_marginTop="@dimen/_16pxv"
                    android:backgroundTint="@color/gray_402"
                    app:fabCustomSize="@dimen/_72pxh"
                    app:srcCompat="@drawable/img_ticket"
                    app:tint="@color/black_900" />

            </RelativeLayout>

        </FrameLayout>

