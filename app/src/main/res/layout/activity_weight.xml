<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="weightVM" type="com.front_end.app.modules.weight.data.viewmodel.WeightVM"/>
    </data>
    <FrameLayout android:id="@+id/frameWeight"
        android:layout_height="match_parent"
        android:layout_width="match_parent" style="@style/groupStylegray_200">
        <ImageView android:id="@+id/image360F247848348"
            android:layout_gravity="start"
            android:layout_height="match_parent"
            android:layout_width="match_parent"
            android:scaleType="fitXY"
            imageRes="@{@drawable/img_360f247848348}"
            tools:ignore="ContentDescription" tools:src="@drawable/img_360f247848348"/>
        <LinearLayout android:id="@+id/linearColumnthree"
            android:layout_gravity="top|center"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_19pxv"
            android:layout_marginEnd="@dimen/_24pxh"
            android:layout_marginStart="@dimen/_24pxh"
            android:layout_marginTop="@dimen/_19pxv"
            android:layout_width="match_parent" android:orientation="vertical">
            <TextView android:ellipsize="end" android:id="@+id/txtThree"
                android:layout_gravity="center_horizontal"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_116pxh"
                android:layout_marginStart="@dimen/_116pxh"
                android:layout_width="wrap_content"
                android:singleLine="true"
                android:text="@{weightVM.weightModel.txtThree}"
                style="@style/txtPoppinsbold24" tools:text="@string/lbl4"/>
            <TextView android:gravity="center_horizontal"
                android:id="@+id/txtWhatsyourCur"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_17pxv"
                android:layout_width="match_parent"
                android:text="@{weightVM.weightModel.txtWhatsyourCur}"
                style="@style/txt_2" tools:text="@string/msg_what_s_your_cur"/>
            <EditText
                android:background="@drawable/rectangle_bg_bluegray_100_radius_19_5"
                android:ellipsize="end"
                android:id="@+id/etRectangleTwentyOne"
                android:layout_gravity="center|start"
                android:gravity="center"
                android:hint="@string/lbl_kg"
                android:layout_height="@dimen/_50pxv"
                android:inputType="number"
                android:textColorHint="@color/gray_703"
                android:layout_marginEnd="@dimen/_115pxh"
                android:layout_marginStart="@dimen/_115pxh"
                android:layout_marginTop="@dimen/_18pxv" android:layout_width="@dimen/_130pxh"/>
            <TextView android:gravity="center_horizontal"
                android:id="@+id/txtWeusethisdat"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_21pxh"
                android:layout_marginStart="@dimen/_21pxh"
                android:layout_marginTop="@dimen/_92pxv"
                android:layout_width="match_parent"
                android:text="@{weightVM.weightModel.txtWeusethisdat}"
                style="@style/txt_3" tools:text="@string/msg_we_use_this_dat"/>
            <LinearLayout android:id="@+id/linearRowback"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_63pxv"
                android:layout_width="match_parent" android:orientation="horizontal">
                <TextView android:ellipsize="end"
                    android:id="@+id/txtBack"
                    android:background="@drawable/rectangle_bg_gray_201_radius_17_5"
                    android:gravity="center"
                    android:paddingHorizontal="@dimen/_3pxh"
                    android:paddingVertical="@dimen/_1pxv"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_5pxv"
                    android:layout_marginStart="@dimen/_41pxh"
                    android:layout_marginTop="@dimen/_4pxv"
                    android:layout_weight="0.52"
                    android:layout_width="10dp" android:singleLine="true"
                    android:text="@{weightVM.weightModel.txtBack}"
                    style="@style/txtPoppinsbold24_1" tools:text="@string/lbl_back"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtNext"
                    android:background="@drawable/rectangle_bg_gray_201_radius_17_5"
                    android:gravity="center"
                    android:paddingHorizontal="@dimen/_5pxh"
                    android:paddingVertical="@dimen/_1pxv"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_2pxv"
                    android:layout_marginEnd="@dimen/_46pxh"
                    android:layout_marginStart="@dimen/_25pxh"
                    android:layout_marginTop="@dimen/_5pxv"
                    android:layout_weight="0.52"
                    android:layout_width="10dp" android:singleLine="true"
                    android:text="@{weightVM.weightModel.txtNext}"
                    style="@style/txtPoppinsbold24_1" tools:text="@string/lbl_next"/>
            </LinearLayout>
        </LinearLayout>
    </FrameLayout>
</layout>
