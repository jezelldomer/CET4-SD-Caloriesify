<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="bmiVM" type="com.front_end.app.modules.bmi.data.viewmodel.BmiVM" />
    </data>
    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

    <LinearLayout android:gravity="center_horizontal"
        android:id="@+id/linearColumnrectangletwentyfive"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:scaleType="fitXY"
        app:imageRes="@{@drawable/img_rectangle25}"
        tools:ignore="ContentDescription"
        tools:src="@drawable/img_rectangle25"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <FrameLayout android:gravity="start"
            android:id="@+id/frameStackrectangletwentyfive"
            android:layout_height="match_parent"
            android:layout_width="match_parent" style="@style/groupStylegray_200">
            <ImageView android:id="@+id/imageRectangleTwentyFive"
                android:layout_gravity="start"
                android:layout_height="match_parent"
                android:layout_width="match_parent"
                android:scaleType="fitXY"
                imageRes="@{@drawable/img_rectangle25}"
                tools:ignore="ContentDescription" tools:src="@drawable/img_rectangle25"/>
            <LinearLayout android:id="@+id/linearColumnuser"
                android:layout_gravity="center"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_1pxh"
                android:layout_marginTop="@dimen/_22pxv"
                android:layout_width="match_parent" android:orientation="vertical">
                <ImageButton
                    android:background="@drawable/rectangle_bg_bluegray_100_radius_28_5"
                    android:id="@+id/btnUser"
                    android:layout_gravity="end"
                    android:layout_height="@dimen/_57pxh"
                    android:layout_marginEnd="@dimen/_13pxh"
                    android:layout_marginStart="@dimen/_13pxh"
                    android:layout_width="@dimen/_57pxh"
                    android:paddingHorizontal="@dimen/_12pxh"
                    android:paddingVertical="@dimen/_12pxv"
                    android:scaleType="fitCenter"
                    android:src="@drawable/img_user_black_900" tools:ignore="ContentDescription"/>
                <LinearLayout android:gravity="center_horizontal"
                    android:id="@+id/linearColumngoodmorningus"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_35pxv"
                    android:layout_width="match_parent"
                    android:orientation="vertical" style="@style/groupStylewhite_1">
                    <TextView android:gravity="center_horizontal"
                        android:id="@+id/txtGoodMorningUS"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_14pxh"
                        android:layout_marginStart="@dimen/_14pxh"
                        android:layout_marginTop="@dimen/_17pxv"
                        android:layout_width="match_parent"
                        android:text="@{bmiVM.bmiModel.txtGoodMorningUS}"
                        style="@style/txt_5" tools:text="@string/msg_good_morning_us"/>
                    <TextView android:gravity="center_horizontal"
                        android:id="@+id/txtLetsstartwit"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_14pxh"
                        android:layout_marginStart="@dimen/_14pxh"
                        android:layout_width="match_parent"
                        android:text="@{bmiVM.bmiModel.txtLetsstartwit}"
                        style="@style/txt_6" tools:text="@string/msg_let_s_start_wit"/>
                    <LinearLayout android:id="@+id/linearRowvector"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_12pxh"
                        android:layout_marginStart="@dimen/_15pxh"
                        android:layout_marginTop="@dimen/_31pxv"
                        android:layout_width="match_parent" android:orientation="horizontal">
                        <LinearLayout
                            android:gravity="center_horizontal"
                            android:id="@+id/linearColumn"
                            android:layout_height="wrap_content"
                            android:layout_weight="0.5"
                            android:layout_width="0dp"
                            android:orientation="vertical" style="@style/groupStylebluegray_1">
                            <LinearLayout
                                android:gravity="center_horizontal"
                                android:id="@+id/linearColumnvector"
                                android:layout_height="match_parent"
                                android:layout_marginBottom="@dimen/_28pxv"
                                android:layout_marginEnd="@dimen/_30pxh"
                                android:layout_marginStart="@dimen/_30pxh"
                                android:layout_marginTop="@dimen/_28pxv"
                                android:layout_width="match_parent"
                                android:orientation="vertical" style="@style/groupStyle">
                                <ImageView android:id="@+id/imageVector"
                                    android:layout_gravity="center_horizontal"
                                    android:layout_height="@dimen/_20pxh"
                                    android:layout_marginEnd="@dimen/_22pxh"
                                    android:layout_marginStart="@dimen/_22pxh"
                                    android:layout_marginTop="@dimen/_10pxv"
                                    android:layout_width="@dimen/_33pxh"
                                    android:scaleType="fitXY"
                                    android:src="@drawable/img_vector_black_900"
                                    tools:ignore="ContentDescription" tools:src="@drawable/img_vector_black_900"/>
                                <TextView android:ellipsize="end"
                                    android:gravity="center_horizontal"
                                    android:id="@+id/txtJournal"
                                    android:layout_height="wrap_content"
                                    android:layout_marginBottom="@dimen/_1pxv"
                                    android:layout_marginEnd="@dimen/_22pxh"
                                    android:layout_marginStart="@dimen/_22pxh"
                                    android:layout_marginTop="@dimen/_12pxv"
                                    android:layout_width="match_parent"
                                    android:singleLine="true"

                                    style="@style/txtPoppinsbold20" tools:text="@string/lbl_journal"/>
                            </LinearLayout>
                        </LinearLayout>
                        <LinearLayout
                            android:id="@+id/linearColumnvolume"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="@dimen/_25pxh"
                            android:layout_weight="0.5"
                            android:layout_width="0dp"
                            android:orientation="vertical" style="@style/groupStylebluegray_1">
                            <ImageView android:id="@+id/imageVolume"
                                android:layout_gravity="center_horizontal"
                                android:layout_height="@dimen/_33pxh"
                                android:layout_marginEnd="@dimen/_72pxh"
                                android:layout_marginStart="@dimen/_72pxh"
                                android:layout_marginTop="@dimen/_31pxv"
                                android:layout_width="@dimen/_26pxh"
                                android:scaleType="fitXY"
                                android:src="@drawable/img_volume"
                                tools:ignore="ContentDescription" tools:src="@drawable/img_volume"/>
                            <TextView android:ellipsize="end"
                                android:id="@+id/txtDiet"
                                android:layout_gravity="center_horizontal"
                                android:layout_height="wrap_content"
                                android:layout_marginBottom="@dimen/_28pxv"
                                android:layout_marginEnd="@dimen/_72pxh"
                                android:layout_marginStart="@dimen/_72pxh"
                                android:layout_marginTop="@dimen/_6pxv"
                                android:layout_width="wrap_content"
                                android:singleLine="true"
                                style="@style/txt_7" tools:text="@string/lbl_diet"/>
                        </LinearLayout>


                    </LinearLayout>

                    <LinearLayout android:id="@+id/linearRowfire"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_12pxh"
                        android:layout_marginStart="@dimen/_15pxh"
                        android:layout_marginTop="@dimen/_31pxv"
                        android:layout_width="match_parent" android:orientation="horizontal">
                        <LinearLayout
                            android:gravity="center_horizontal"
                            android:id="@+id/linearColumnfire"
                            android:layout_height="wrap_content"
                            android:layout_weight="0.5"
                            android:paddingVertical="@dimen/_2pxv"
                            android:layout_width="0dp"
                            android:orientation="vertical" style="@style/groupStylebluegray_1">
                            <ImageView android:id="@+id/imageFire"
                                android:layout_gravity="center_horizontal"
                                android:layout_height="@dimen/_36pxh"
                                android:layout_marginEnd="@dimen/_55pxh"
                                android:layout_marginStart="@dimen/_55pxh"
                                android:layout_marginTop="@dimen/_26pxv"
                                android:layout_width="@dimen/_30pxh"
                                android:scaleType="fitXY"
                                android:src="@drawable/img_fire"
                                tools:ignore="ContentDescription" tools:src="@drawable/img_fire"/>
                            <TextView android:ellipsize="end"
                                android:id="@+id/txtWorkout"
                                android:layout_gravity="center_horizontal"
                                android:layout_height="wrap_content"
                                android:layout_marginBottom="@dimen/_30pxv"
                                android:layout_marginEnd="@dimen/_55pxh"
                                android:layout_marginStart="@dimen/_55pxh"
                                android:layout_marginTop="@dimen/_9pxv"
                                android:layout_width="wrap_content"
                                android:singleLine="true"
                                style="@style/txt_8" tools:text="@string/lbl_workout"/>
                        </LinearLayout>
                        <FrameLayout android:id="@+id/frameStackvector"
                            android:layout_height="@dimen/_146pxv"
                            android:layout_marginStart="@dimen/_29pxh"
                            android:layout_width="@dimen/_192pxh"
                            android:layout_marginTop="@dimen/_1pxv">
                        <LinearLayout
                            android:id="@+id/linearColumnfavorite"
                            android:layout_gravity="bottom|start"
                            android:layout_height="wrap_content"
                            android:paddingVertical="@dimen/_40pxv"
                            android:layout_marginEnd="@dimen/_10pxh"
                            android:layout_width="match_parent"
                            android:layout_marginBottom="@dimen/_1pxv"
                            android:orientation="vertical" style="@style/groupStylebluegray_1">
                            <ImageView android:id="@+id/imageFavorite"
                                android:layout_height="match_parent"
                                android:layout_marginBottom="@dimen/_2pxv"
                                android:layout_marginEnd="@dimen/_30pxh"
                                android:layout_marginStart="@dimen/_30pxh"
                                android:layout_marginTop="@dimen/_2pxv"
                                android:layout_width="match_parent"
                                android:src="@drawable/img_favorite"
                                tools:ignore="ContentDescription" tools:src="@drawable/img_favorite"/>
                            <TextView android:ellipsize="end"
                                android:id="@+id/txtBMI"
                                android:gravity="center_horizontal"
                                android:layout_height="wrap_content"
                                android:layout_marginEnd="@dimen/_22pxh"
                                android:layout_marginStart="@dimen/_22pxh"
                                android:layout_width="match_parent"
                                android:singleLine="true"
                                style="@style/txt_9" tools:text="@string/lbl_bmi"/>
                        </LinearLayout>
                            <ImageButton
                                android:background="@drawable/rectangle_bg_white_a700_radius_16_5"
                                android:id="@+id/btnCheckmark"
                                android:layout_gravity="top|end"
                                android:layout_height="@dimen/_33pxh"
                                android:layout_marginBottom="@dimen/_10pxv"
                                android:layout_marginStart="@dimen/_10pxh"
                                android:layout_width="@dimen/_33pxh"
                                android:paddingHorizontal="@dimen/_5pxh"
                                android:paddingVertical="@dimen/_5pxv"
                                android:scaleType="fitCenter"
                                android:src="@drawable/img_checkmark" tools:ignore="ContentDescription"/>
                        </FrameLayout>
                    </LinearLayout>
                    <LinearLayout android:gravity="center_horizontal"
                        android:id="@+id/linearColumnairplane"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_12pxh"
                        android:layout_marginStart="@dimen/_14pxh"
                        android:layout_marginTop="@dimen/_31pxv"
                        android:layout_width="match_parent"
                        android:orientation="vertical" style="@style/groupStylebluegray_1">
                        <ImageView android:id="@+id/imageAirplane"
                            android:layout_gravity="end"
                            android:layout_height="@dimen/_41pxh"
                            android:layout_marginEnd="@dimen/_169pxh"
                            android:layout_marginStart="@dimen/_169pxh"
                            android:layout_marginTop="@dimen/_19pxv"
                            android:layout_width="@dimen/_41pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_airplane"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_airplane"/>
                        <TextView android:ellipsize="end"
                            android:gravity="end"
                            android:id="@+id/txtCreateyourCus"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="@dimen/_25pxv"
                            android:layout_marginEnd="@dimen/_46pxh"
                            android:layout_marginStart="@dimen/_46pxh"
                            android:layout_marginTop="@dimen/_13pxv"
                            android:layout_width="match_parent"
                            android:singleLine="true"
                            android:text="@{bmiVM.bmiModel.txtCreateyourCus}"
                            style="@style/txt_10" tools:text="@string/msg_create_your_cus"/>
                    </LinearLayout>
                    <androidx.appcompat.widget.AppCompatButton
                        android:ellipsize="end" android:gravity="center"
                        android:id="@+id/btnProceed"
                        android:layout_gravity="center_horizontal"
                        android:layout_height="@dimen/_60pxv"
                        android:layout_marginBottom="@dimen/_80pxv"
                        android:layout_marginEnd="@dimen/_14pxh"
                        android:layout_marginStart="@dimen/_14pxh"
                        android:layout_marginTop="@dimen/_20pxv"
                        android:layout_width="@dimen/_346pxh"
                        android:paddingBottom="@dimen/_12pxv"
                        android:paddingEnd="@dimen/_30pxh"
                        android:paddingStart="@dimen/_30pxh"
                        android:paddingTop="@dimen/_12pxv"
                        android:singleLine="true"
                        android:text="@string/lbl_proceed"
                        android:textAllCaps="false"
                        style="@style/btnSolidRounded_1" tools:text="@string/lbl_proceed"/>
                </LinearLayout>
            </LinearLayout>
        </FrameLayout>
    </LinearLayout>
    </androidx.constraintlayout.widget.ConstraintLayout>
</layout>
