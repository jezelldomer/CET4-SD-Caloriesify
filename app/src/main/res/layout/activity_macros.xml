<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools">

    <data>
        <variable
                name="dietVM"
                type="com.front_end.app.modules.diet.data.viewmodel.DietVM" />
    </data>

    <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

        <LinearLayout
                android:id="@+id/linearColumnrectangletwentyfive"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginTop="@dimen/_1pxv"
                android:orientation="vertical"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                style="@style/groupStylegray_200">

            <FrameLayout
                    android:id="@+id/frameStackrectangletwentyfive"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    style="@style/groupStylegray_200"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent">

                <ImageView
                        android:id="@+id/imageRectangleTwentyFive"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_rectangle25"
                        tools:ignore="ContentDescription,tools:src"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent" />

                <LinearLayout
                        android:id="@+id/linearColumnuser"
                        android:layout_width="match_parent"
                        android:layout_height="729dp"
                        android:layout_gravity="center"
                        android:layout_marginEnd="@dimen/_1pxh"
                        android:layout_marginTop="@dimen/_22pxv"
                        android:orientation="vertical"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent">

                    <ImageButton
                            android:id="@+id/btnUser"
                            android:layout_width="@dimen/_57pxh"
                            android:layout_height="@dimen/_57pxh"
                            android:layout_gravity="end"
                            android:layout_marginEnd="@dimen/_13pxh"
                            android:layout_marginStart="@dimen/_13pxh"
                            android:background="@drawable/rectangle_bg_bluegray_100_radius_28_5"
                            android:paddingHorizontal="@dimen/_12pxh"
                            android:paddingVertical="@dimen/_12pxv"
                            android:scaleType="fitCenter"
                            android:src="@drawable/img_user_black_900"
                            tools:ignore="ContentDescription"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="parent"/>

                    <LinearLayout
                            android:id="@+id/linearColumngoodmorningus"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/_32pxv"
                            android:layout_marginBottom="@dimen/_22pxv"
                            android:orientation="vertical"
                            style="@style/groupStylewhite_1"
                            app:layout_constraintBottom_toBottomOf="parent"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toBottomOf="@id/btnUser">

                        <TextView
                                android:id="@+id/txtGoodMorningUS"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginEnd="@dimen/_14pxh"
                                android:layout_marginStart="@dimen/_14pxh"
                                android:layout_marginTop="@dimen/_17pxv"
                                android:gravity="center_horizontal"
                                android:text="@{dietVM.dietModel.txtGoodMorningUS}"
                                style="@style/txt_5"
                                tools:text="@string/msg_good_morning_us"/>

                        <TextView
                                android:id="@+id/txtLetsstartwit"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginEnd="@dimen/_14pxh"
                                android:layout_marginStart="@dimen/_65pxh"
                                android:text="@{dietVM.dietModel.txtLetsstartwit}"
                                style="@style/txt_6"
                                tools:text="@string/msg_let_s_start_wit"/>

                        <LinearLayout
                                android:id="@+id/linearRowvector"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginEnd="@dimen/_12pxh"
                                android:layout_marginStart="@dimen/_15pxh"
                                android:layout_marginTop="@dimen/_31pxv"
                                android:orientation="horizontal"
                                app:layout_constraintBottom_toBottomOf="parent"
                                app:layout_constraintEnd_toEndOf="parent"
                                app:layout_constraintStart_toStartOf="parent"
                                app:layout_constraintTop_toBottomOf="@id/txtLetsstartwit">

                            <LinearLayout
                                    android:id="@+id/linearColumn"
                                    android:layout_width="0dp"
                                    android:layout_height="wrap_content"
                                    android:layout_weight="0.5"
                                    android:orientation="vertical"
                                    style="@style/groupStylebluegray_1"
                                    app:layout_constraintBottom_toBottomOf="parent"
                                    app:layout_constraintEnd_toEndOf="parent"
                                    app:layout_constraintStart_toStartOf="parent"
                                    app:layout_constraintTop_toTopOf="parent">

                                <LinearLayout
                                        android:id="@+id/linearColumnvector"
                                        android:layout_width="match_parent"
                                        android:layout_height="match_parent"
                                        android:layout_marginBottom="@dimen/_28pxv"
                                        android:layout_marginEnd="@dimen/_30pxh"
                                        android:layout_marginStart="@dimen/_30pxh"
                                        android:layout_marginTop="@dimen/_28pxv"
                                        android:orientation="vertical"
                                        style="@style/groupStyle"
                                        app:layout_constraintBottom_toBottomOf="parent"
                                        app:layout_constraintEnd_toEndOf="parent"
                                        app:layout_constraintStart_toStartOf="parent"
                                        app:layout_constraintTop_toTopOf="parent">

                                    <ImageView
                                            android:id="@+id/imageVector"
                                            android:layout_width="@dimen/_33pxh"
                                            android:layout_height="@dimen/_20pxh"
                                            android:layout_gravity="center_horizontal"
                                            android:layout_marginEnd="@dimen/_22pxh"
                                            android:layout_marginStart="@dimen/_22pxh"
                                            android:layout_marginTop="@dimen/_10pxv"
                                            android:scaleType="fitXY"
                                            android:src="@drawable/img_vector_black_900"
                                            tools:ignore="ContentDescription"/>

                                    <TextView
                                            android:id="@+id/txtJournal"
                                            android:layout_width="match_parent"
                                            android:layout_height="wrap_content"
                                            android:layout_marginBottom="@dimen/_1pxv"
                                            android:layout_marginEnd="@dimen/_22pxh"
                                            android:layout_marginStart="@dimen/_22pxh"
                                            android:layout_marginTop="@dimen/_12pxv"
                                            android:ellipsize="end"
                                            android:gravity="center_horizontal"
                                            android:singleLine="true"
                                            android:text="@{dietVM.dietModel.txtJournal}"
                                            style="@style/txtPoppinsbold20"
                                            tools:text="@string/lbl_journal"/>

                                </LinearLayout>
                            </LinearLayout>

                            <LinearLayout
                                    android:id="@+id/linearColumnvolume"
                                    android:layout_width="0dp"
                                    android:layout_height="wrap_content"
                                    android:layout_marginStart="@dimen/_25pxh"
                                    android:layout_weight="0.5"
                                    android:orientation="vertical"
                                    style="@style/groupStylebluegray_1"
                                    app:layout_constraintBottom_toBottomOf="parent"
                                    app:layout_constraintEnd_toEndOf="parent"
                                    app:layout_constraintStart_toEndOf="@id/linearColumn"
                                    app:layout_constraintTop_toTopOf="parent">

                                <ImageView
                                        android:id="@+id/imageVolume"
                                        android:layout_width="@dimen/_26pxh"
                                        android:layout_height="@dimen/_33pxh"
                                        android:layout_gravity="center_horizontal"
                                        android:layout_marginEnd="@dimen/_72pxh"
                                        android:layout_marginStart="@dimen/_72pxh"
                                        android:layout_marginTop="@dimen/_31pxv"
                                        android:scaleType="fitXY"
                                        android:src="@drawable/img_volume"
                                        tools:ignore="ContentDescription"/>

                                <TextView
                                        android:id="@+id/txtDiet"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_gravity="center_horizontal"
                                        android:layout_marginBottom="@dimen/_28pxv"
                                        android:layout_marginEnd="@dimen/_72pxh"
                                        android:layout_marginStart="@dimen/_72pxh"
                                        android:layout_marginTop="@dimen/_6pxv"
                                        android:ellipsize="end"
                                        android:singleLine="true"
                                        android:text="@{dietVM.dietModel.txtDiet}"
                                        style="@style/txt_7"
                                        tools:text="@string/lbl_diet"/>

                            </LinearLayout>

                        </LinearLayout>

                        <LinearLayout
                                android:id="@+id/linearRowfire"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginEnd="@dimen/_12pxh"
                                android:layout_marginStart="@dimen/_15pxh"
                                android:layout_marginTop="@dimen/_31pxv"
                                android:orientation="horizontal"
                                app:layout_constraintBottom_toBottomOf="parent"
                                app:layout_constraintEnd_toEndOf="parent"
                                app:layout_constraintStart_toStartOf="parent"
                                app:layout_constraintTop_toBottomOf="@id/linearRowvector">

                            <LinearLayout
                                    android:id="@+id/linearColumnfire"
                                    android:layout_width="0dp"
                                    android:layout_height="wrap_content"
                                    android:layout_weight="0.5"
                                    android:orientation="vertical"
                                    style="@style/groupStylebluegray_1"
                                    app:layout_constraintBottom_toBottomOf="parent"
                                    app:layout_constraintEnd_toEndOf="parent"
                                    app:layout_constraintStart_toStartOf="parent"
                                    app:layout_constraintTop_toTopOf="parent">

                                <ImageView
                                        android:id="@+id/imageFire"
                                        android:layout_width="@dimen/_30pxh"
                                        android:layout_height="@dimen/_36pxh"
                                        android:layout_gravity="center_horizontal"
                                        android:layout_marginEnd="@dimen/_55pxh"
                                        android:layout_marginStart="@dimen/_55pxh"
                                        android:layout_marginTop="@dimen/_26pxv"
                                        android:scaleType="fitXY"
                                        android:src="@drawable/img_fire"
                                        tools:ignore="ContentDescription"/>

                                <TextView
                                        android:id="@+id/txtWorkout"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_gravity="center_horizontal"
                                        android:layout_marginBottom="@dimen/_30pxv"
                                        android:layout_marginEnd="@dimen/_55pxh"
                                        android:layout_marginStart="@dimen/_55pxh"
                                        android:layout_marginTop="@dimen/_9pxv"
                                        android:ellipsize="end"
                                        android:singleLine="true"
                                        android:text="@{dietVM.dietModel.txtWorkout}"
                                        style="@style/txt_8"
                                        tools:text="@string/lbl_workout"/>

                            </LinearLayout>

                            <LinearLayout
                                    android:id="@+id/linearColumnfavorite"
                                    android:layout_width="0dp"
                                    android:layout_height="wrap_content"
                                    android:layout_marginStart="@dimen/_25pxh"
                                    android:layout_weight="0.5"
                                    android:orientation="vertical"
                                    style="@style/groupStylebluegray_1"
                                    app:layout_constraintBottom_toBottomOf="parent"
                                    app:layout_constraintEnd_toEndOf="parent"
                                    app:layout_constraintStart_toEndOf="@id/linearColumnfire"
                                    app:layout_constraintTop_toTopOf="parent">

                                <ImageView
                                        android:id="@+id/imageFavorite"
                                        android:layout_width="@dimen/_33pxh"
                                        android:layout_height="@dimen/_29pxh"
                                        android:layout_gravity="center_horizontal"
                                        android:layout_marginEnd="@dimen/_75pxh"
                                        android:layout_marginStart="@dimen/_75pxh"
                                        android:layout_marginTop="@dimen/_29pxv"
                                        android:scaleType="fitXY"
                                        android:src="@drawable/img_favorite"
                                        tools:ignore="ContentDescription"/>

                                <TextView
                                        android:id="@+id/txtBMI"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_gravity="center_horizontal"
                                        android:layout_marginBottom="@dimen/_30pxv"
                                        android:layout_marginEnd="@dimen/_75pxh"
                                        android:layout_marginStart="@dimen/_75pxh"
                                        android:layout_marginTop="@dimen/_13pxv"
                                        android:ellipsize="end"
                                        android:singleLine="true"
                                        android:text="@{dietVM.dietModel.txtBMI}"
                                        style="@style/txt_9"
                                        tools:text="@string/lbl_bmi"/>

                            </LinearLayout>

                        </LinearLayout>

                        <FrameLayout
                                android:id="@+id/linearColumnairplane"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginEnd="@dimen/_12pxh"
                                android:layout_marginStart="@dimen/_14pxh"
                                android:layout_marginTop="@dimen/_31pxv"
                                android:orientation="vertical"
                                style="@style/groupStylebluegray_1"
                                app:layout_constraintBottom_toBottomOf="parent"
                                app:layout_constraintEnd_toEndOf="parent"
                                app:layout_constraintStart_toStartOf="parent"
                                app:layout_constraintTop_toBottomOf="@id/linearRowfire">

                            <ImageButton
                                    android:background="@drawable/rectangle_bg_white_a700_radius_16_5"
                                    android:id="@+id/btnCheckmark"
                                    android:layout_gravity="top|end"
                                    android:layout_height="@dimen/_33pxh"
                                    android:layout_width="@dimen/_33pxh"
                                    android:paddingHorizontal="@dimen/_5pxh"
                                    android:paddingVertical="@dimen/_5pxv"
                                    android:scaleType="fitCenter"
                                    android:src="@drawable/img_checkmark" tools:ignore="ContentDescription"
                                    android:translationX="@dimen/_6pxh"/>

                            <ImageView
                                    android:id="@+id/imageAirplane"
                                    android:layout_width="@dimen/_41pxh"
                                    android:layout_height="@dimen/_41pxh"
                                    android:layout_gravity="end"
                                    android:layout_marginEnd="@dimen/_169pxh"
                                    android:layout_marginStart="@dimen/_169pxh"
                                    android:layout_marginTop="@dimen/_19pxv"
                                    android:scaleType="fitXY"
                                    android:src="@drawable/img_airplane"
                                    tools:ignore="ContentDescription"/>

                            <TextView
                                    android:id="@+id/txtCreateyourCus"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_gravity="end"
                                    android:layout_marginBottom="@dimen/_25pxv"
                                    android:layout_marginEnd="@dimen/_46pxh"
                                    android:layout_marginStart="@dimen/_46pxh"
                                    android:layout_marginTop="@dimen/_80pxv"
                                    android:ellipsize="end"
                                    android:gravity="end"
                                    android:singleLine="true"
                                    android:text="@{dietVM.dietModel.txtCreateyourCus}"
                                    style="@style/txt_10"
                                    tools:text="@string/msg_create_your_cus"/>

                        </FrameLayout>
                        <androidx.appcompat.widget.AppCompatButton
                                android:id="@+id/btnProceed"
                                android:layout_width="@dimen/_346pxh"
                                android:layout_height="@dimen/_60pxv"
                                android:layout_marginBottom="@dimen/_80pxv"
                                android:layout_marginEnd="@dimen/_14pxh"
                                android:layout_marginStart="@dimen/_45pxh"
                                android:layout_marginTop="@dimen/_20pxv"
                                android:gravity="center"
                                android:paddingBottom="@dimen/_12pxv"
                                android:paddingEnd="@dimen/_30pxh"
                                android:paddingStart="@dimen/_30pxh"
                                android:paddingTop="@dimen/_12pxv"
                                android:singleLine="true"
                                android:text="@string/lbl_proceed"
                                android:textAllCaps="false"
                                style="@style/btnSolidRounded_1"
                                tools:text="@string/lbl_proceed"
                                android:layout_alignParentTop="true"
                                android:layout_centerHorizontal="true"
                        />

                    </LinearLayout>


                </LinearLayout>

            </FrameLayout>

        </LinearLayout>

    </RelativeLayout>
</layout>
