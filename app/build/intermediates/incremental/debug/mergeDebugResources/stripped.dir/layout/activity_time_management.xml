<?xml version="1.0" encoding="UTF-8"?>

                                                                                                       
    
                                                                                                                           
           
    <LinearLayout android:id="@+id/linearTimemanagement"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylegray_200" android:tag="layout/activity_time_management_0" xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
        <androidx.appcompat.widget.Toolbar
            android:gravity="center_horizontal"
            android:id="@+id/toolbarToolbar"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_12pxh"
            android:layout_marginStart="@dimen/_12pxh"
            android:layout_marginTop="@dimen/_18pxv"
            android:layout_width="match_parent" app:contentInsetStart="0dp">
            <LinearLayout android:id="@+id/linearRowarrowfive"
                android:layout_height="wrap_content"
                android:layout_width="match_parent" android:orientation="horizontal">
                <FrameLayout android:id="@+id/frameStackarrowfive"
                    android:layout_height="@dimen/_39pxv"
                    android:layout_marginTop="@dimen/_6pxv"
                    android:layout_width="@dimen/_41pxh" style="@style/groupStylegreen_A700cornerRadius">
                    <ImageView android:id="@+id/imageArrowFive"
                        android:layout_gravity="bottom|start"
                        android:layout_height="@dimen/_3pxh"
                        android:layout_marginBottom="@dimen/_17pxv"
                        android:layout_marginEnd="@dimen/_15pxh"
                        android:layout_marginStart="@dimen/_15pxh"
                        android:layout_marginTop="@dimen/_17pxv"
                        android:layout_width="@dimen/_1pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_arrow5"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_arrow5"/>
                </FrameLayout>
                <ImageButton
                    android:background="@drawable/rectangle_bg_white_a700_radius_16_5"
                    android:id="@+id/btnCheckmark"
                    android:layout_height="@dimen/_33pxh"
                    android:layout_marginBottom="@dimen/_12pxv"
                    android:layout_marginStart="@dimen/_328pxh"
                    android:layout_width="@dimen/_33pxh"
                    android:paddingHorizontal="@dimen/_5pxh"
                    android:paddingVertical="@dimen/_5pxv"
                    android:scaleType="fitCenter"
                    android:src="@drawable/img_checkmark" tools:ignore="ContentDescription"/>
            </LinearLayout>
        </androidx.appcompat.widget.Toolbar>
        <LinearLayout android:id="@+id/linearRowarrowleft"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_12pxh"
            android:layout_marginStart="@dimen/_8pxh"
            android:layout_marginTop="@dimen/_23pxv"
            android:layout_width="match_parent" android:orientation="horizontal">
            <ImageView android:id="@+id/imageArrowleft"
                android:layout_height="@dimen/_20pxh"
                android:layout_marginBottom="@dimen/_1pxv"
                android:layout_marginTop="@dimen/_1pxv"
                android:layout_width="@dimen/_11pxh"
                android:scaleType="fitXY"
                android:src="@drawable/img_arrowleft"
                tools:ignore="ContentDescription" tools:src="@drawable/img_arrowleft"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtUntitled"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_163pxh"
                android:layout_weight="0.8" android:layout_width="0dp"
                android:singleLine="true"
                android:tag="binding_1"                                           
                style="@style/txtRobotobold18" tools:text="@string/lbl_untitled"/>
            <ImageView android:id="@+id/imageFrameTen"
                android:layout_height="@dimen/_20pxh"
                android:layout_marginBottom="@dimen/_1pxv"
                android:layout_marginStart="@dimen/_163pxh"
                android:layout_marginTop="@dimen/_1pxv"
                android:layout_width="@dimen/_4pxh"
                android:scaleType="fitXY"
                android:src="@drawable/img_iconhamburge"
                tools:ignore="ContentDescription" tools:src="@drawable/img_iconhamburge"/>
        </LinearLayout>
        <TextView android:ellipsize="end" android:id="@+id/txtTime"
            android:layout_gravity="start"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_20pxh"
            android:layout_marginStart="@dimen/_20pxh"
            android:layout_marginTop="@dimen/_43pxv"
            android:layout_width="wrap_content"
            android:singleLine="true"
            android:tag="binding_2"                                       
            style="@style/txtRobotomedium18" tools:text="@string/lbl_time"/>
        <TextView android:ellipsize="end" android:id="@+id/txtActivity"
            android:layout_gravity="start"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_20pxh"
            android:layout_marginStart="@dimen/_20pxh"
            android:layout_marginTop="@dimen/_7pxv"
            android:layout_width="wrap_content"
            android:singleLine="true"
            android:tag="binding_3"                                           
            style="@style/txtRobotomedium18" tools:text="@string/lbl_activity"/>
        <TextView android:ellipsize="end" android:id="@+id/txtWith"
            android:layout_gravity="start"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_20pxv"
            android:layout_marginEnd="@dimen/_22pxh"
            android:layout_marginStart="@dimen/_22pxh"
            android:layout_marginTop="@dimen/_3pxv"
            android:layout_width="wrap_content"
            android:singleLine="true"
            android:tag="binding_4"                                       
            style="@style/txtRobotomedium18" tools:text="@string/lbl_with"/>
    </LinearLayout>
         